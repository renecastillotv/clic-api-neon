{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "clic-api-neon",
  "regions": ["iad1"],
  "functions": {
    "index.ts": {
      "runtime": "edge",
      "memory": 128,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.ts"
    }
  ]
}
